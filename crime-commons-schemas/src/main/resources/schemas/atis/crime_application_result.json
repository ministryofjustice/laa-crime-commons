{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "crime_application_result.json",
  "type": "object",
  "properties": {
    "usn": {
      "type": "integer",
      "nullable": true,
      "description": "USN number"
    },
    "maat_ref": {
      "type": "integer",
      "nullable": true,
      "description": "MAAT / Rep Id"
    },
    "case_id": {
      "type": "string",
      "nullable": true,
      "description": "Case ID"
    },
    "case_type": {
      "type": "string",
      "nullable": true,
      "enum": ["INDICTABLE", "SUMMARY ONLY", "CC ALREADY", "APPEAL CC", "COMMITAL", "EITHER WAY"]
    },
    "ioj_result": {
      "type": "string",
      "nullable": true
    },
    "ioj_reason": {
      "type": "string",
      "nullable": true,
      "description": "IOJ Notes"
    },
    "ioj_assessor_name": {
      "type": "string",
      "nullable": true
    },
    "app_created_date": {
      "type": "string",
      "format": "date-time",
      "nullable": true,
      "description": "Date of application created"
    },
    "means_result": {
      "type": "string",
      "nullable": true,
      "description": "Means Assessment Result"
    },
    "means_assessor_name": {
      "type": "string",
      "nullable": true,
      "description": "Means Assessment Assessor Name"
    },
    "date_means_created": {
      "type": "string",
      "format": "date-time",
      "nullable": true,
      "description": "Means Assessment Created Date"
    },
    "funding_decision": {
      "type": "string",
      "nullable": true,
      "description": "Funding Decision"
    },
    "cc_rep_decision": {
      "type": "string",
      "nullable": true,
      "description": "CC Rep Order Decision"
    },
    "ioj_appeal_result": {
      "type": "string",
      "nullable": true,
      "description": "IoJ Appeal Result"
    },
    "ioj_appeal_assessor_name": {
      "type": "string",
      "nullable": true,
      "description": "IoJ Appeal Assessor Name"
    },
    "ioj_appeal_date": {
      "type": "string",
      "format": "date-time",
      "nullable": true,
      "description": "IoJ Appeal Date"
    },
    "passport_result": {
      "type": "string",
      "nullable": true,
      "description": "Passported Result"
    },
    "passport_assessor_name": {
      "type": "string",
      "nullable": true,
      "description": "Passported Assessor Name"
    },
    "date_passport_created": {
      "type": "string",
      "format": "date-time",
      "nullable": true,
      "description": "Passported Created Date"
    },
    "passport_review_type": {
      "type": "string",
      "nullable": true,
      "description": "Passported Review Type"
    },
    "means_review_type": {
      "type": "string",
      "nullable": true,
      "description": "Means Assessment Review Type"
    },
    "passport_work_reason": {
      "type": "string",
      "nullable": true,
      "description": "Passported New Work Reason Code"
    },
    "means_work_reason": {
      "type": "string",
      "nullable": true,
      "description": "Means Assessment New Work Reason Code"
    }
  }
}
